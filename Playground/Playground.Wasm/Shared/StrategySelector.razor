@using Playground.Wasm.Services
@using MaterialColorUtilities.Palettes
@inject ThemeService ThemeService

<MudSelect T="CorePalette.Strategy" @bind-Value="@ThemeService.Strategy" Label="Strategy"
           AnchorOrigin="Origin.BottomCenter"
           Variant="Variant.Filled">
    <MudSelectItem Value="CorePalette.Strategy.Default" />
    <MudSelectItem Value="CorePalette.Strategy.Content" />
    <MudSelectItem Value="CorePalette.Strategy.A" />
    <MudSelectItem Value="CorePalette.Strategy.B" />
    <MudSelectItem Value="CorePalette.Strategy.C" />
</MudSelect>
